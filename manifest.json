{
  "name": "mocker",
  "description": "api mock tool",
  "version": "0.0.1",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "file://*/*",
    "<all_urls>",
    "tabs",
    "storage"
  ],
  "background": {
    "page": "src/background.html"
  },
  "browser_action": {
    "default_icon": "assets/debuggerContinue.png",
    "default_title": "mocker"
  },
  "manifest_version": 2
}
