{
  "name": "mocker",
  "description": "api mock tool",
  "version": "0.1",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "tabs",
    "debugger"
  ],
  "background": {
    "scripts": [
      "mock.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "assets/debuggerContinue.png",
    "default_title": "mocker",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
